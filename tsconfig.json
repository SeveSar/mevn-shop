{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "isolatedModules": false,
    "lib": ["es5","es2017", "es6", "dom", "dom.iterable"]
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": ["off"]
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
